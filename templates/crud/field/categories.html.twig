{# @var field \EasyCorp\Bundle\EasyAdminBundle\Dto\FieldDto #}
{# @var entity \EasyCorp\Bundle\EasyAdminBundle\Dto\EntityDto #}
{% for value in field.formattedValue %}
<span class="badge badge-secondary" style="isolation: isolate;z-index: 2; z-index:2; background-color:{{ value.color }};">
